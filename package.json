{
  "name": "tab-system",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "cra-template": "1.2.0",
    "marked": "^16.0.0",
    "pdf-lib": "^1.17.1",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:fast": "FAST_REFRESH=true react-scripts start",
    "dev": "npm run start:fast",
    "build": "react-scripts build",
    "build:analyze": "npm run build && npx serve -s build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "performance:audit": "npm run build && npx lighthouse http://localhost:3000 --view",
    "size:analyze": "npm run build && npx bundlesize",
    "clear-cache": "rm -rf node_modules/.cache && rm -rf build",
    "fresh-start": "npm run clear-cache && npm start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11"
  }
}
